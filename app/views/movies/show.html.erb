<h1>Movie: <%= @movie.title %></h1>

<br>

<iframe src="<%= @movie.trailer %>" frameborder="0" allowfullscreen></iframe>

<p>comments:</p>
<ul>
  <% @movie.comments.each do |comment| %>
    <li>
         <%= link_to comment.body %>
      [<%= link_to comment.user_id %>]  
    </li>
  <% end %>
</ul>