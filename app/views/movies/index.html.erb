<h1>Movies#index</h1>

<%= link_to 'Create a new move', new_movie_path %>
<br>

<ul>
  <% @movies.each do |movie| %>
    <li>
      <%= link_to movie.title, movie_path(movie) %>
      - [ <%= link_to 'edit', edit_movie_path(movie) %>
          <%= link_to 'delete', movie_path(movie), method: :delete%>
      ]
    </li>
  <% end %>
</ul>
